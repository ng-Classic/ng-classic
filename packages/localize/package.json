{
  "name": "@angular-classic/localize",
  "version": "0.0.0-PLACEHOLDER",
  "description": "Angular Classic - library for localizing messages",
  "bin": {
    "localize-translate": "./tools/bundles/src/translate/cli.js",
    "localize-extract": "./tools/bundles/src/extract/cli.js",
    "localize-migrate": "./tools/bundles/src/migrate/cli.js"
  },
  "exports": {
    "./tools": {
      "types": "./tools/index.d.ts",
      "default": "./tools/bundles/index.js"
    }
  },
  "author": "angular",
  "license": "MIT",
  "engines": {
    "node": "^16.14.0 || >=18.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ng-classic/ng-classic.git"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "packageGroup": "NG_UPDATE_PACKAGE_GROUP"
  },
  "ng-add": {
    "save": "devDependencies"
  },
  "sideEffects": [
    "./esm2022/init/**",
    "./fesm2022/init.mjs"
  ],
  "dependencies": {
    "@babel/core": "7.21.8",
    "fast-glob": "3.2.12",
    "yargs": "^17.2.1"
  },
  "peerDependencies": {
    "@angular-classic/compiler": "0.0.0-PLACEHOLDER",
    "@angular-classic/compiler-cli": "0.0.0-PLACEHOLDER"
  }
}
